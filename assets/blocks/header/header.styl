header
	.header
		height: 125px
		.container
			.header-wrap
				height: 125px
				display: flex
				.header-blocks
					align-self: center
					width: 100%
					height: 53px
					position: relative
					.logo
						display: block
						width: 174px
						height: 53px
						margin: 0 auto
					.icon-wrapp
						position: absolute
						right: 0
						top: 7px
						.icons
							width: 129px
							img
								display: inline-block
								border-radius: 15px
								height: 30px
								width: 30px
								border: 1px solid #ebebeb
								color: #43a6c7
								text-align: center
								padding-top: 8px
								margin-left: 13px
								transition: all .5s
								&:hover
									color: white
									background-color: #43a6c7
									border-color: #43a6c7
@media (max-width: $screen-sm-min )
	.header
		height: 140px
	.icon-wrapp
		right: auto !important
		top: auto !important
		bottom: -35px !important
		width: 100%
		.icons
			margin: 0 auto