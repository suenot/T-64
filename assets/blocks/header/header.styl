.header
	height: 125px
	.container
		.header-wrap
			height: 125px
			display: flex
			.header-blocks
				align-self: center
				width: 100%
				height: 53px
				position: relative
				.logo
					display: block
					width: 174px
					height: 53px
					margin: 0 auto
				.icons
					//align-self: center
					position: absolute
					right: 0
					top: 7px
					i
						display: inline-block
						border-radius: 15px
						height: 30px
						width: 30px
						border: 1px solid #ebebeb
						color: #43a6c7
						text-align: center
						padding-top: 8px
						margin-left: 13px
						transition: all .5s
						&:hover
							color: white
							background-color: #43a6c7